  <library_images>
    <image id="ShadowImage" name="ShadowImage">
      <init_from>ShadowImage.png</init_from>
    </image>
  </library_images>
  <library_effects>
    <effect id="ShadowMaterial-effect">
      <profile_COMMON>
        <newparam sid="ShadowImage-surface">
          <surface type="2D">
            <init_from>ShadowImage</init_from>
          </surface>
        </newparam>
        <newparam sid="ShadowImage-sampler">
          <sampler2D>
            <source>ShadowImage-surface</source>
          </sampler2D>
        </newparam>
        <technique sid="common">
          <phong>
            <emission>
              <color sid="emission">0 0 0 1</color>
            </emission>
            <ambient>
              <color sid="ambient">0 0 0 1</color>
            </ambient>
            <diffuse>
              <texture texture="ShadowImage-sampler"/>
            </diffuse>
            <specular>
              <color sid="specular">0.5 0.5 0.5 1</color>
            </specular>
            <shininess>
              <float sid="shininess">50</float>
            </shininess>
            <index_of_refraction>
              <float sid="index_of_refraction">1</float>
            </index_of_refraction>
          </phong>
        </technique>
      </profile_COMMON>
    </effect>
  </library_effects>
  <library_materials>
    <material id="ShadowMaterial-material" name="ShadowMaterial">
      <instance_effect url="#ShadowMaterial-effect"/>
    </material>
  </library_materials>
  <library_geometries>
    <geometry id="Plane-mesh" name="Plane">
      <mesh>
        <source id="Plane-mesh-positions">
